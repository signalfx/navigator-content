{
  "id": "cf diego",
  "unreleased": true,
  "displayName": "Diego",
  "category": "Pivotal Cloud Foundry",
  "categoryPriority": 30,
  "type": "architectural",
  "alertQuery": "metric_source:cloudfoundry AND job:(diego_cell diego_brain diego_database)",
  "requiredProperties": [
    "host"
  ],
  "mtsQuery": "metric_source:cloudfoundry AND job:(diego_cell diego_brain diego_database)",
  "systemDashboardPrefix": "Cloud Foundry",
  "discoveryQuery": [
    "sf_key:job"
  ],
  "propertyColumns": [
    [
      {
        "header": "Tags",
        "properties": [
          "sf_tags"
        ]
      },
      {
        "header": "Other",
        "properties": []
      },
      {
        "header": "AWS",
        "properties": []
      }
    ]
  ],
  "listColumns": [
    {
      "property": "id",
      "displayName": "Instance",
      "format": "id"
    },
    {
      "property": "value",
      "displayName": "Value",
      "format": "Number"
    },
    {
      "property": "job",
      "displayName": "Job"
    }
  ],
  "tooltipKeyList": [
    {
      "property": "id",
      "displayName": "Instance",
      "isSummaryProperty": true
    },
    {
      "property": "value",
      "displayName": "Value",
      "isSummaryProperty": true,
      "format": "Number"
    },
    {
      "property": "job",
      "displayName": "Job",
      "isSummaryProperty": true
    }
  ],
  "map": {
    "job": {
      "resolution": 300000,
      "filters": [],
      "template": "DIEGO_CELL = data(\"rep.UnhealthyCell\", filter=filter(\"metric_source\", \"cloudfoundry\") and filter(\"job\", \"diego_cell\"){{#filter}} and {{{filter}}}{{/filter}}, extrapolation=\"last_value\", maxExtrapolations=2).mean(by=[\"host\"])\nDIEGO_BRAIN = data(\"system.healthy\", filter=filter(\"metric_source\", \"cloudfoundry\") and filter(\"job\", \"diego_brain\"){{#filter}} and {{{filter}}}{{/filter}}, extrapolation=\"last_value\", maxExtrapolations=2).mean(by=[\"host\"])\nDIEGO_DATABASE = data(\"system.healthy\", filter=filter(\"metric_source\", \"cloudfoundry\") and filter(\"job\", \"diego_database\"){{#filter}} and {{{filter}}}{{/filter}}, extrapolation=\"last_value\", maxExtrapolations=2).mean(by=[\"host\"])"
    },
    "groupingStructure": {
      "key": "sf_streamLabel",
      "drawBoundary": true
    },
    "metrics": [
      {
        "id": "cf.diego_cell.healthy",
        "name": "Cells",
        "varName": "DIEGO_CELL",
        "valueLabel": "Cell Unhealthy",
        "idTemplate": "{{host}}",
        "coloring": {
          "method": "threshold",
          "minValue": 0,
          "maxValue": 1,
          "limits": [
            {
              "lowerLimit": 1,
              "color": "#ea1849"
            },
            {
              "lowerLimit": 0,
              "color": "#05ce00"
            }
          ]
        }
      },
      {
        "id": "cf.diego_brain.healthy",
        "name": "Brains",
        "varName": "DIEGO_BRAIN",
        "valueLabel": "System Health",
        "idTemplate": "{{host}}",
        "coloring": {
          "method": "threshold",
          "minValue": 0,
          "maxValue": 1,
          "limits": [
            {
              "lowerLimit": 1,
              "color": "#05ce00"
            },
            {
              "lowerLimit": 0,
              "color": "#ea1849"
            }
          ]
        }
      },
      {
        "id": "cf.diego_database.healthy",
        "name": "Databases",
        "varName": "DIEGO_DATABASE",
        "valueLabel": "System Health",
        "idTemplate": "{{host}}",
        "coloring": {
          "method": "threshold",
          "minValue": 0,
          "maxValue": 1,
          "limits": [
            {
              "lowerLimit": 1,
              "color": "#05ce00"
            },
            {
              "lowerLimit": 0,
              "color": "#ea1849"
            }
          ]
        }
      }
    ]
  }
}
