{
  "id": "azure compute",
  "displayName": "Virtual Machines",
  "category": "Azure",
  "categoryPriority": 20,
  "type": "elemental",
  "alertQuery": "_exists_:resource_id",
  "requiredProperties": [
    "resource_name"
  ],
  "idTemplate": "{{resource_name}}",
  "idName": "AzureCompute",
  "mtsQuery": "sf_metric:\"Percentage CPU\" AND aggregation_type:average AND resource_provider_namespace:Microsoft.Compute",
  "systemDashboardPrefix": "Virtual Machine",
  "discoveryQuery": ["sf_key:resource_id" ],
  "propertyColumns": [
    [
      {
        "header": "Other",
        "properties": []
      }
    ]
  ],
  "listColumns": [
    {
      "property": "id",
      "displayName": "Instance Name"
    },
    {
      "property": "value",
      "displayName": "Value",
      "format": "Number"
    },
    {
      "property": "resource_group_id",
      "displayName": "Group ID"
    },
    {
      "property": "region",
      "displayName": "Region"
    }
  ],
  "tooltipKeyList": [
    {
      "property": "resource_name",
      "displayName": "Name",
      "isSummaryProperty": true
    },
    {
      "property": "value",
      "displayName": "Value",
      "isSummaryProperty": true,
      "format": "Number"
    }
  ],
  "metrics": [
    {
      "id": "azure.percentage_cpu",
      "type": "metric",
      "displayName": "CPU Utilization",
      "metricSelectors": [
        "Percentage CPU"
      ],
      "description": "Color hosts based on percentage of CPU being used: under 20% (green) to over 80% (red)",
      "valueLabel": "CPU Use",
      "valueFormat": "Percentage",
      "job": {
        "resolution": 300000,
        "filters": [
          {
            "property": "resource_provider_namespace",
            "propertyValue": "Microsoft.Compute",
            "type": "property"
          },
          {
            "property": "aggregation_type",
            "propertyValue": "average",
            "type": "property"
          }
        ],
        "template": "CPU_UTILIZATION = data(\"Percentage CPU\"{{#filter}}, filter={{{filter}}}{{/filter}}, extrapolation=\"last_value\", maxExtrapolations=5)",
        "varName": "CPU_UTILIZATION"
      },
      "coloring": {
        "method": "quantile",
        "minValue": 0,
        "maxValue": 100
      }
    },
    {
      "id": "azure.network_in",
      "type": "metric",
      "displayName": "Network Received Bytes",
      "metricSelectors": [
        "Network In"
      ],
      "description": "Color hosts based on network bytes in per second",
      "valueLabel": "Bytes In",
      "valueFormat": "Bytes",
      "job": {
        "resolution": 300000,
        "filters": [
          {
            "property": "aggregation_type",
            "propertyValue": "total",
            "type": "property"
          }
        ],
        "template": "NETWORK_IN = data(\"Network In\"{{#filter}}, filter={{{filter}}}{{/filter}}, extrapolation=\"last_value\", maxExtrapolations=2)",
        "varName": "NETWORK_IN"
      },
      "coloring": {
        "method": "quantile",
        "minValue": 0,
        "range": [
          "#bfd3e6",
          "#9ebcda",
          "#8c96c6",
          "#8856a7",
          "#810f7c"
        ]
      }
    },
    {
      "id": "azure.network_out",
      "type": "metric",
