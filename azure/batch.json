{
  "id": "azurebatch",
  "unreleased": true,
  "displayName": "Batch",
  "category": "Azure",
  "categoryPriority": 50,
  "type": "elemental",
  "alertQuery": "_exists_:azure_resource_id",
  "requiredProperties": [
    "azure_resource_name"
  ],
  "idTemplate": "{{azure_resource_name}}",
  "idName": "azure_batch_service",
  "mtsQuery": "resource_type:Microsoft.Batch\\/batchAccounts",
  "systemDashboardPrefix": "Azure Batch",
  "discoveryQuery": [
    "resource_type:Microsoft.Batch/batchAccounts"
  ],
  "propertyColumns": [
    [
      {
        "header": "Other",
        "properties": []
      }
    ]
  ],
  "listColumns": [
    {
      "property": "azure_resource_name",
      "displayName": "Resource Name"
    },
    {
      "property": "value",
      "displayName": "Value",
      "format": "Number"
    },
    {
      "property": "azure_resource_group_name",
      "displayName": "Group"
    },
    {
      "property": "azure_region",
      "displayName": "Region"
    }
  ],
  "tooltipKeyList": [
    {
      "property": "id",
      "displayName": "ID",
      "isSummaryProperty": true
    },
    {
      "property": "value",
      "displayName": "Value",
      "isSummaryProperty": true,
      "format": "Number"
    }
  ],
  "metrics": [
    {
      "id": "azure.batch.taskscompleted",
      "type": "metric",
      "displayName": "Tasks Completed",
      "metricSelectors": [
        "TaskCompleteEvent"
      ],
      "description": "Color hosts by number of tasks completed in past 5 minutes",
      "valueLabel": "Tasks Completed",
      "valueFormat": "Number",
      "job": {
        "resolution": 300000,
        "filters": [
          {
            "property": "resource_type",
            "propertyValue": "Microsoft.Batch/batchAccounts",
            "type": "property"
          },
          {
            "property": "primary_aggregation_type",
            "propertyValue": "true"
          }
        ],
        "template": "TASKS_COMPLETED = data(\"TaskCompleteEvent\", filter={{#filter}}{{{filter}}}{{/filter}}, rollup=\"sum\", extrapolation=\"null\", maxExtrapolations=-1).mean(by=[\"azure_resource_name\", \"azure_resource_group_name\", \"azure_region\"])",
        "varName": "TASKS_COMPLETED"
      },
      "coloring": {
        "method": "quantile",
        "minValue": 0,
        "range": [
          "#bfd3e6",
          "#9ebcda",
          "#8c96c6",
          "#8856a7",
          "#810f7c"
        ]
      }
    },
    {
