{
  "id": "kubernetes architecture",
  "unreleased": false,
  "type": "architectural",
  "displayName": "Architecture",
  "alertQuery": "_exists_:kubernetes_cluster",
  "mtsQuery": "sf_metric:cpu.utilization AND _exists_:kubernetes_cluster",
  "category": "Kubernetes",
  "categoryPriority": 10,
  "systemDashboardPrefix": "K8s Cluster",
  "discoveryQuery": [
    "_exists_:kubernetes_cluster"
  ],
  "propertyColumns": [
    [
      {
        "header": "Tags",
        "properties": [
          "sf_tags"
        ]
      },
      {
        "header": "Other",
        "properties": []
      },
      {
        "header": "AWS",
        "properties": []
      }
    ]
  ],
  "listColumns": [
    {
      "property": "id",
      "displayName": "Instance",
      "format": "id"
    },
    {
      "property": "value",
      "displayName": "Value",
      "format": "Number"
    },
    {
      "property": "kubernetes_cluster",
      "displayName": "Cluster"
    }
  ],
  "tooltipKeyList": [
    {
      "property": "id",
      "displayName": "Instance",
      "isSummaryProperty": true
    },
    {
      "property": "value",
      "displayName": "Value",
      "isSummaryProperty": true,
      "format": "Number"
    },
    {
      "property": "kubernetes_cluster",
      "displayName": "Cluster",
      "isSummaryProperty": true
    }
  ],
  "map": {
    "job": {
      "resolution": 60000,
      "filters": [],
      "template": "NODES = data(\"cpu.utilization\", filter=filter(\"kubernetes_cluster\", \"*\") and filter(\"kubernetes_role\", \"worker\"){{#filter}} and {{{filter}}}{{/filter}}, extrapolation=\"last_value\", maxExtrapolations=2).mean(by=[\"host\", \"kubernetes_cluster\"])\nMASTERS = data(\"cpu.utilization\", filter=filter(\"kubernetes_cluster\", \"*\") and filter(\"kubernetes_role\", \"master\"){{#filter}} and {{{filter}}}{{/filter}}, extrapolation=\"last_value\", maxExtrapolations=2).mean(by=[\"host\", \"kubernetes_cluster\"])"
    },
    "groupingStructure": {
      "key": "kubernetes_cluster",
      "drawBoundary": true,
      "children": [
        {
          "key": "sf_streamLabel",
          "memberSettings": [
            {
              "name": "Masters",
              "bigger": true
            },
            {
              "name": "Nodes",
              "bigger": true
            }
          ]
        }
      ]
    },
    "metrics": [
      {
        "name": "Nodes",
        "varName": "NODES",
        "valueLabel": "CPU Use",
        "valueFormat": "Percentage",
        "idTemplate": "{{host}}",
        "coloring": {
          "method": "quantile",
          "minValue": 0,
          "maxValue": 100
        }
      },
      {
        "name": "Masters",
        "varName": "MASTERS",
        "valueLabel": "CPU Use",
        "valueFormat": "Percentage",
        "idTemplate": "{{host}}",
        "coloring": {
          "method": "quantile",
          "minValue": 0,
          "maxValue": 100
        }
      }
    ]
  }
}
