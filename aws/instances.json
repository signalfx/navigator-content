{
  "id": "AWS instances",
  "displayName": "EC2 Instances",
  "category": "AWS",
  "categoryPriority": 50,
  "type": "elemental",
  "alertQuery": "_exists_:InstanceId",
  "requiredProperties": [
    "InstanceId"
  ],
  "proxyProperties": [
    "aws_instance_id"
  ],
  "idTemplate": "{{InstanceId}}",
  "idName": "Instance",
  "mtsQuery": "sf_metric:CPUUtilization AND sf_key:InstanceId AND _exists_:InstanceId",
  "systemDashboardPrefix": "EC2",
  "discoveryQuery": ["sf_key:InstanceId"],
  "propertyColumns": [
    [
      {
        "header": "AWS",
        "properties": []
      }
    ],
    [
      {
        "header": "Tags",
        "properties": [
          "sf_tags"
        ]
      },
      {
        "header": "Other",
        "properties": []
      }
    ]
  ],
  "listColumns": [
    {
      "property": "id",
      "displayName": "Instance ID",
      "format": "id"
    },
    {
      "property": "value",
      "displayName": "Value",
      "format": "Number"
    },
    {
      "property": "aws_tag_Name",
      "displayName": "Name"
    },
    {
      "property": "aws_availability_zone",
      "displayName": "Availability Zone"
    },
    {
      "property": "aws_instance_type",
      "displayName": "Instance Type"
    },
    {
      "property": "aws_state",
      "displayName": "State",
      "format": "aws_state"
    }
  ],
  "tooltipKeyList": [
    {
      "property": "aws_tag_Name",
      "displayName": "Name",
      "isSummaryProperty": true
    },
    {
      "property": "value",
      "displayName": "Value",
      "isSummaryProperty": true,
      "format": "Number"
    },
    {
      "property": "host_collect_version",
      "displayName": "collectd version"
    },
    {
      "property": "host_kernel_name",
      "displayName": "OS"
    },
    {
      "property": "host_mem_total",
      "displayName": "Memory",
      "format": "Kilobytes"
    },
    {
      "property": "host_logical_cpus",
      "displayName": "vCPU"
    }
  ],
  "metrics": [
    {
      "id": "aws.CPUUtilization",
      "type": "metric",
      "displayName": "CPUUtilization",
      "metricSelectors": [
        "CPUUtilization"
      ],
      "description": "Color hosts based on percentage of CPU being used: under 20% (green) to over 80% (red)",
      "valueLabel": "CPU Use",
      "valueFormat": "Percentage",
      "job": {
        "resolution": 300000,
        "filters": [
          {
            "property": "_exists_",
            "propertyValue": "InstanceId",
            "type": "property"
          },
          {
            "property": "stat",
            "propertyValue": "mean",
            "type": "property"
          }
        ],
        "template": "CPU_UTILIZATION = data(\"CPUUtilization\"{{#filter}}, filter={{{filter}}}{{/filter}}, extrapolation=\"last_value\", maxExtrapolations=2).mean(by=[\"InstanceId\"])",
        "varName": "CPU_UTILIZATION"
      },
      "coloring": {
        "method": "quantile",
        "minValue": 0,
        "maxValue": 100
      }
    },
    {
      "id": "aws.NetworkIn",
      "type": "metric",
      "displayName": "NetworkIn",
      "metricSelectors": [
        "NetworkIn"
      ],
      "description": "Color hosts based on network bytes in per second",
      "job": {
        "resolution": 300000,
        "filters": [
          {
            "property": "_exists_",
            "propertyValue": "InstanceId",
            "type": "property"
          },
          {
            "property": "stat",
            "propertyValue": "sum",
            "type": "property"
          }
        ],
        "template": "NETWORK_IN_A = data(\"NetworkIn\"{{#filter}}, filter={{{filter}}}{{/filter}}, extrapolation=\"last_value\", maxExtrapolations=2).sum(by=[\"InstanceId\"])\nNETWORK_IN = (NETWORK_IN_A * 60)",
        "varName": "NETWORK_IN"
      },
      "coloring": {
        "method": "quantile",
        "minValue": 0
      }
    },
    {
      "id": "aws.NetworkOut",
      "type": "metric",
